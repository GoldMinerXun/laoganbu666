<template name="tempcomment">
  <!-- <navigator hover-class='none' open-type='navigateTo' url='../q_second/index?id={{qid}}'> -->
    <view class='tempcomment-wrap' bindtap='navigateToUrl' data-qid='{{qid}}'>
      <view class='tempcommment-avator'>
        <image class='tempcommment-avator-img' src="{{commentAvatarUrl}}"></image>
      </view>
      <view class='tempcomment-text-wrap'>
        <view class='tempcomment-name-time'>
          <view class='tempcomment-name'>{{commentNickName}}</view>
          <view class='tempcomment-time'>{{time}}</view>
        </view>
        <view class='tempcomment-showcontent'>回复我：{{ccontent}}</view>
        <view class='tempcomment-mycontent'>我的问题:{{title}}</view>
        <view class='tempcomment-line'></view>
      </view>
    </view>
  <!-- </navigator> -->

</template>